<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
				   assembly="ToDoApp"
				   namespace="ToDoApp.model">
	
	<class name="Todo">
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="Name"/>
		<property name="Text"/>
	</class>	
</hibernate-mapping>